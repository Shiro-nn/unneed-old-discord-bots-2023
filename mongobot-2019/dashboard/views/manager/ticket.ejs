            $(document).ready(function () {
                console.log(document.getElementsByClassName("changecolor"));
                let array = document.getElementsByClassName("changecolor");
                for (let index = 0; index < array.length; index++) {
                    const element = array[index];
                    let color = decimalToHexString(element.id);
                    element.style.color = color;
                    console.log(element);
                    console.log(element.id);
                    console.log(element.style.color);
                    console.log(color);
                }
            });
            function decimalToHexString(c) {
                var b = c % 256,
                g_0 = (c % 65536 - b),
                r_0 = c - g_0 - b,
                g = g_0 / 256,
                r = r_0 / 65536;
                return `rgb(${r}, ${g}, ${b})`;
            }
